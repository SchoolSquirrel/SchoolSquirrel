{
    "name": "SchoolSquirrel",
    "nativescript": {
        "id": "com.schoolsquirrel.SchoolSquirrel",
        "tns-android": {
            "version": "6.5.1"
        },
        "tns-ios": {
            "version": "6.5.1"
        }
    },
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "eslint . --ext .ts",
        "lint-fix": "eslint . --ext .ts --fix",
        "e2e": "ng e2e",
        "clean": "npx rimraf hooks node_modules platforms package-lock.json && npm i",
        "android": "tns debug android --env.aot --no-hmr",
        "ios": "tns debug ios --env.aot --no-hmr",
        "mobile": "tns run --env.aot --no-hmr",
        "preview": "tns preview --env.aot --no-hmr",
        "desktop": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron ./ --serve",
        "electron:tsc": "tsc main.ts",
        "ngcc": "ngcc --properties es2015 module main --first-only",
        "postinstall": "npm run ngcc"
    },
    "main": "main.js",
    "private": true,
    "dependencies": {
        "@angular/animations": "^9.1.11",
        "@angular/cdk": "~8.2.3",
        "@angular/common": "^9.1.11",
        "@angular/compiler": "^9.1.11",
        "@angular/core": "^9.1.11",
        "@angular/forms": "^9.1.11",
        "@angular/localize": "^9.1.11",
        "@angular/material": "^8.2.3",
        "@angular/platform-browser": "^9.1.11",
        "@angular/platform-browser-dynamic": "^9.1.11",
        "@angular/router": "^9.1.11",
        "@fortawesome/fontawesome-free": "^5.13.1",
        "@nativescript/angular": "^9.0.0",
        "@nativescript/core": "^6.5.7",
        "@nativescript/theme": "^2.3.3",
        "@ng-bootstrap/ng-bootstrap": "^6.1.0",
        "@ngx-translate/core": "^12.1.2",
        "@ngx-translate/http-loader": "^5.0.0",
        "@syncfusion/ej2-angular-dropdowns": "^18.1.57",
        "@syncfusion/ej2-angular-grids": "^18.1.57",
        "@syncfusion/ej2-angular-schedule": "^18.1.56",
        "bootstrap": "^4.5.0",
        "cldr-data": "^36.0.0",
        "core-js": "^3.6.5",
        "electron": "^9.0.4",
        "electron-push-receiver": "^2.1.3",
        "electron-reload": "^1.5.0",
        "electron-splashscreen": "^1.0.0",
        "reflect-metadata": "~0.1.13",
        "rxjs": "~6.5.5",
        "tslib": "2.0.0",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.901.9",
        "@angular/cli": "^9.1.9",
        "@angular/compiler-cli": "^9.1.11",
        "@nativescript/schematics": "^9.0.0",
        "@types/jasmine": "~3.5.10",
        "@types/jasminewd2": "~2.0.8",
        "@types/node": "^14.0.13",
        "@typescript-eslint/eslint-plugin": "^3.3.0",
        "@typescript-eslint/parser": "^3.3.0",
        "codelyzer": "^5.2.2",
        "copy-webpack-plugin": "~5.1.1",
        "eslint": "^7.2.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-import-resolver-typescript": "^2.0.0",
        "eslint-plugin-github": "^4.0.1",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-json": "^2.1.1",
        "eslint-plugin-jsx-a11y": "^6.3.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.0",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.2",
        "karma": "~5.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~3.3.1",
        "karma-jasmine-html-reporter": "^1.5.4",
        "nativescript-dev-webpack": "^1.5.1",
        "node-sass": "^4.14.1",
        "protractor": "~7.0.0",
        "terser-webpack-plugin": "~2.2.0",
        "ts-node": "~8.10.2",
        "tslint": "~6.1.2",
        "typescript": "~3.8.3",
        "wait-on": "^5.0.1"
    }
}
