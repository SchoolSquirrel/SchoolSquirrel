{
    "name": "SchoolSquirrel",
    "version": "0.0.1",
    "description": "SchoolSquirrel - Die Platform für digitalen Unterricht. Mit Chat, Aufgaben, Video-Konferenzen und mehr!",
    "author": {
        "name": "Hannes Rüger",
        "url": "https://hannesrueger.de"
    },
    "scripts": {
        "web:start": "ng serve",
        "web:build": "ng build --prod",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "clean:crazy": "npx rimraf hooks node_modules platforms package-lock.json && npm i",
        "clean": "npx rimraf hooks platforms package-lock.json",
        "mobile:android:run": "tns debug android --env.aot --no-hmr",
        "mobile:android:build": "tns build android --env.aot",
        "mobile:ios:run": "tns debug ios --env.aot --no-hmr",
        "mobile:ios:build": "tns build ios --env.aot",
        "desktop:run": "wait-on http-get://localhost:4200/ && npm run _electron:tsc && electron ./ --serve",
        "desktop:build": "npm run web:build && npm run _electron:tsc && npx electron-builder build",
        "ngcc": "ngcc --properties es2015 module main --first-only",
        "_electron:tsc": "tsc electron-main.ts && npx move-cli electron-main.js main.js",
        "postinstall": "npm run ngcc"
    },
    "main": "main.js",
    "private": true,
    "nativescript": {
        "id": "com.schoolsquirrel.schoolsquirrel"
    },
    "dependencies": {
