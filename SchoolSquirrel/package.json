        "@nativescript-community/ui-material-floatingactionbutton": "^5.1.16",
        "@nativescript/angular": "^11.0.0",
        "@nativescript/core": "^7.1.0",
        "@nativescript/firebase": "^11.1.3",
        "@nativescript/local-notifications": "^5.1.0",
